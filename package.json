{
  "name": "notes-app",
  "version": "0.0.0",
  "description": "Web app for note taking",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "start:ui": "cd packages/notes-frontend && ember s",
    "start:api": "cd packages/notes-backend && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel --presets es2015 -d dist/ src",
    "build:ember": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start:ember": "ember serve --proxy http://localhost:8080",
    "test:ember": "ember test"
  },
