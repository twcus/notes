{
  "name": "notes-app",
  "version": "1.4.0",
  "description": "Web app for note taking",
  "main": "dist/app.js",
  "scripts": {
    "start:api": "yarn run build:api && node dist/app.js",
    "build:api": "babel --presets es2015 -d dist/ src",
    "start:ui": "ember serve --proxy http://localhost:8080",
    "build:ui": "ember build --environment=production --output-path=dist/public",
    "test:ui": "ember test -s",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "npm-run-all lint test:*",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "heroku-postbuild": "yarn run build:ui"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/twcus/notes-app.git"
  },
  "author": "Thomas Custer",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/twcus/notes-app/issues"
  },
  "engines": {
    "node": "12.* || 14.* || 16.x"
  },
  "homepage": "https://gitlab.com/twcus/notes-app#README",
  "dependencies": {
    "babel": "^6.23.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
