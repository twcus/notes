    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "fortune": "^5.5.15",
    "fortune-http": "^1.2.25",
    "fortune-json-api": "^2.1.5",
    "fortune-postgres": "^1.6.10",
    "jsonwebtoken": "^8.5.1",
    "node-postgres": "^0.6.2",
    "signale": "^1.4.0"
  },
